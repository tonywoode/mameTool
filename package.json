{
  "name": "mametool",
  "version": "1.0.0",
  "description": "",
  "main": "src/QPNode.js",
  "directories": {
    "outputDir": "./outputs",
    "binOutputDir": "/Volumes/Untitled/Emulators/QUICKPLAY/QuickPlayFrontend/qp/bin",
    "binOutputDirParallels": "/Volumes/C/Emulators/QUICKPLAY/QuickPlayFrontend/qp/bin"
  },
  "config": {
    "devFlags": "--dev",
    "getSpecs": "find specs -name '*spec.js'"
  },
  "scripts": {
    "base": "node src/QPNode.js",
    "start": "node src/QPNode.js --output-dir $npm_package_directories_outputDir $npm_package_config_devFlags",
    "mess-start": "node src/QPNode --datAndEfind --output-dir $npm_package_directories_outputDir $npm_package_config_devFlags; node src/QPNode --softlists --output-dir $npm_package_directories_outputDir $npm_package_config_devFlags; node src/QPNode --embedded --output-dir $npm_package_directories_outputDir $npm_package_config_devFlags",
    "full": "trash $npm_package_directories_outputDir && mkdir $npm_package_directories_outputDir && npm run start -- --scan",
    "basedebug": "node --inspect-brk src/QPNode.js",
    "debug": "npm run basedebug -- --output-dir $npm_package_directories_outputDir $npm_package_config_devFlags",
    "fulldebug": "trash $npm_package_directories_outputDir && npm run debug --",
    "test": "mocha -- --require ./specs/init.js $(eval $npm_package_config_getSpecs)",
    "package": "pkg src/QPNode.js -t win --out-path $npm_package_directories_binOutputDir",
    "packageParallels": "pkg src/QPNode.js -t win --out-path $npm_package_directories_binOutputDirParallels"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.11.0",
    "daggy": "^1.2.0",
    "fs-extra": "^4.0.2",
    "ini": "^1.3.4",
    "levenshtein": "^1.0.5",
    "mkdirp": "^0.5.1",
    "ramda": "^0.24.1",
    "trash-cli": "^1.4.0",
    "xml-stream": "^0.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "into-stream": "^3.1.0",
    "mocha": "^3.4.2",
    "mock-fs": "^4.4.1",
    "pkg": "^4.2.4",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1"
  }
}
