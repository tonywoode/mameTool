{
  "name": "mametool",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "outputDir": "./outputs",
    "getSpecs": "find specs -name '*spec.js'"
  },
  "scripts": {
    "comment": "a non-full run keeps the mame.json in outputs, a full run deletes the whole outputs folder",
    "debug": "trash $npm_package_config_outputDir/*/ && node --inspect-brk index.js",
    "full": "trash $npm_package_config_outputDir && node index.js",
    "fulldebug": "trash $npm_package_config_outputDir && node --inspect-brk index.js",
    "start": "trash $npm_package_config_outputDir/*/ && node index.js",
    "test": "mocha --require ./specs/init.js $(eval $npm_package_config_getSpecs)"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "daggy": "^1.2.0",
    "ini": "^1.3.4",
    "mkdirp": "^0.5.1",
    "ramda": "^0.24.1",
    "trash-cli": "^1.4.0",
    "xml-stream": "^0.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "into-stream": "^3.1.0",
    "mocha": "^3.4.2",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.4.1"
  }
}
